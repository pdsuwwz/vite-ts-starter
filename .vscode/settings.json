{
  "stylelint.validate": [
    "vue",
		"scss"
  ],
  "css.validate": false,
	"less.validate": false,

	"editor.formatOnSave": false,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit",
		"eslint.autoFixOnSave": "explicit",
		"source.fixAll.stylelint": "explicit"
	},
	"files.autoSaveDelay": 500,
	"vue.codeActions.enabled": false
}
